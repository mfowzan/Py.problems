nums=[]
target=int(input("Enter your target number"))
while True:
    b=input("enter the number you want to add and leave blank to stop")
    if b=='':
        break
    else:
       nums=nums+[int(b)]

a=[]
ind=0
target=9
for i in nums:
    for j in nums:
        if i+j==target:
            a.append(nums.index(i))
            a.append(nums.index(j))
        
print(nums)
print(a)

